@model dynamic

@{
    ViewBag.Title = "Not Found";
    Layout = "_Layout";
}

<!--== Start Faq Area Wrapper ==-->
<section class="page-not-found-area">
    <div class="container pt--0 pb--0">
        <div class="row justify-content-center">
            <div class="col-lg-8">
                <div class="page-not-found-wrap">
                    <div class="page-not-found-thumb">
                        <img src="~/img/photos/404.png" alt="Image">
                    </div>
                    <div class="page-not-found-content">
                        <h2 class="title">Sorry, this page is not found.</h2>
                        <a class="btn-theme" asp-controller="Home" asp-action="Index"><i class="icofont-long-arrow-left"></i> Back to home</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>